{
  "name": "gitconnected",
  "version": "1.0.0",
  "description": "Networking tool for developers",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelRempe/gitConnected.git"
  },
  "author": "Michael J.Rempe",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "bugs": {
    "url": "https://github.com/MichaelRempe/gitConnected/issues"
  },
  "homepage": "https://github.com/MichaelRempe/gitConnected#readme",
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "axios": "^0.18.0",
    "bootstrap": "^4.4.1",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-jwt": "^6.0.0",
    "history": "^4.10.1",
    "if-env": "^1.0.4",
    "jwks-rsa": "^1.6.2",
    "mongoose": "^5.3.16",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.2",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-id-generator": "^3.0.0",
    "react-slick": "^0.25.2"
  }
}
